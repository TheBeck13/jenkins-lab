pipeline{
  agent none
  
  stages{
    stage('Test'){
      agent{
        node {label 'test'}
      }
      steps{
        sh 'echo test'
      }
      
    }
    stage('Deploy'){
      agent{
        node {label 'production'}
      }
      steps{
        sh 'echo production'
      }
      
    }
  }
